@incollection{smithLungIsolationAnesthesia2024,
  title = {Lung Isolation Anesthesia},
  booktitle = {{{StatPearls}}},
  author = {Smith, Sarah and Arthur, Mary E.},
  year = {2024},
  publisher = {StatPearls Publishing},
  address = {Treasure Island (FL)},
  urldate = {2024-08-04},
  abstract = {Lung isolation describes either anatomic or physiologic separation of~1 lung from the other using an advanced airway device, typically a bronchial blocker or double-lumen endotracheal tube. Anatomical lung isolation separates a diseased lung and prevents~contamination of~the healthy lung. For example, in the case of whole lung lavage, anatomical lung isolation is used to avoid spillage of blood or pus from the diseased to the healthy lung. Physiological lung separation ventilates each lung as an independent unit and may be indicated when unilateral lung pathology requires differential ventilatory parameters for each lung. One-lung ventilation is the ability to ventilate~1 lung while allowing the other to collapse to facilitate surgical exposure or manage disease states. The deflation of~1 lung can establish an immobile surgical field and create space within the thorax, allowing access to anatomical structures.~ A~clinician with advanced airway training (ie, anesthesiologist or intensivist) should place the~double-lumen endotracheal~or bronchial blocker. Extensive knowledge of pulmonary physiology is essential for managing lung isolation, particularly during periods of 1-lung ventilation, which can be complicated by hypoxia. When used to facilitate surgery, lung isolation requires clear communication between the surgical and anesthesiology teams to prevent and manage complications. In the intensive care setting, nurses and respiratory therapists must be aware of the physiologic consequences of differential or 1-lung ventilation and detect changes in the patient's condition that may indicate a problem with lung isolation.},
  copyright = {Copyright {\copyright} 2024, StatPearls Publishing LLC.},
  langid = {english},
  lccn = {NBK555933},
  pmid = {32310393},
  file = {/Users/aakash/Zotero/storage/I3AFXKZ6/NBK555933.html}
}

