@article{rollasEffectsEndexpiratoryLung2022,
  title = {Effects of End-Expiratory Lung Volume versus {{PaO2}} Guided {{PEEP}} Determination on Respiratory Mechanics and Oxygenation in Moderate to Severe {{ARDS}}},
  author = {Rollas, Kazim and Hanci, Pervin and Topeli, Arzu},
  year = {2022},
