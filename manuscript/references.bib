@article{huiWearableRFNearField2021,
  title = {Wearable {{RF Near-Field Cough Monitoring}} by {{Frequency-Time Deep Learning}}},
  author = {Hui, Xiaonan and Zhou, Jianlin and Sharma, Pragya and Conroy, Thomas B. and Zhang, Zijing and Kan, Edwin C.},
  year = {2021},
  month = aug,
  journal = {IEEE Transactions on Biomedical Circuits and Systems},
  volume = {15},
  number = {4},
  pages = {756--764},
  issn = {1940-9990},
  doi = {10.1109/TBCAS.2021.3099865},
  urldate = {2024-08-09},
  abstract = {Coughing is a common symptom for many respiratory disorders, and can spread droplets of various sizes containing bacterial and viral pathogens. Mild coughs are usually overlooked in the early stage, not only because they are barely noticeable by the person and the people around, but also because the present recording method is not comfortable, private, or reliable for long-term monitoring. In this paper, a wearable radio-frequency (RF) sensor is presented to recognize the mild cough signal directly from the local trachea vibration characteristics, and can isolate interferences from nearby people. The sensor operates at the ultra-high-frequency band, and can couple the RF energy to the upper respiratory track by the near field of the sensing antenna. The retrieved tissue vibration caused by the cough airflow burst can then be analyzed by a convolutional neural network trained on the frequency-time spectra. The sensing antenna design is analyzed for performance improvement. During the human study of 5 participants over 100 minutes of prescribed routines, the overall recognition ratio is above 90\% and the false positive ratio during other routines is below 2.09\%.},
  keywords = {Antennas,Biomedical signal processing,Dipole antennas,Load flow,machine learning,microwave sensors,Monitoring,near-field radiation pattern,Radio frequency,Sensors,Vibrations,wearable devices},
  file = {/Users/aakash/Zotero/storage/ATVYDK4P/9496122.html}
}

@article{mansouriElectricalImpedanceTomography2021,
