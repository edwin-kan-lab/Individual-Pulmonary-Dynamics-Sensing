  author = {Edginton, Stefan and Kruger, Natalia and Stelfox, Henry Tom and Brochard, Laurent and Zuege, Danny J. and Gaudet, Jonathan and Solverson, Kevin J. and Robertson, Helen Lee and Fiest, Kirsten M. and Niven, Daniel J. and Bagshaw, Sean M. and Parhar, Ken Kuljit S.},
  year = {2023},
  month = jul,
  journal = {BMJ Open},
  volume = {13},
